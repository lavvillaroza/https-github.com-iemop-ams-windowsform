            SQL = "SELECT * FROM AM_WESM_BILL " & _
                  "WHERE INVOICE_DATE BETWEEN " & _
                  "TO_DATE('" & StartOfMonth & "','MM/DD/YYYY') " & _
                  "AND TO_DATE('" & EndOfMonth & "','MM/DD/YYYY') " & _
                  "AND ID_NUMBER IN ("

