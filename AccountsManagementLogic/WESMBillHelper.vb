                                    & "SUM(CASE WHEN CHARGE_TYPE = 'E' THEN ENDING_BALANCE ELSE 0 END) ENERGY, " & vbNewLine _
                                    & "SUM(CASE WHEN CHARGE_TYPE = 'EV' THEN ENDING_BALANCE ELSE 0 END) VAT, " & vbNewLine _
