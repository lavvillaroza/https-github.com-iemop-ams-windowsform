        Dim SQL As String = ""

        Try
            Select Case TransType
                Case EnumPrudentialTransType.Replenishment
                    SQL = "SELECT a.*, b.participant_id, b.full_name " & _
                          "FROM AM_PRUDENTIAL_HISTORY a, AM_PARTICIPANTS b " & _
