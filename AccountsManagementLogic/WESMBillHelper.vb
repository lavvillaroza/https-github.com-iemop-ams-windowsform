                End With
            Next

            dtWESMBillSummary.AcceptChanges()

            'Add the tables in dataset
            With ds.Tables
                .Add(dtJV)
                .Add(dtJVDetails)
                .Add(dtDMCM)
                .Add(dtDMCMDetails)
                .Add(dtOffset)
                .Add(dtWESMBillSummary)
